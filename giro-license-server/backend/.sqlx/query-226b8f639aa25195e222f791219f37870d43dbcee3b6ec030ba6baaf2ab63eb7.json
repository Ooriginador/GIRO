{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT DISTINCT\n                h.id, h.fingerprint, h.machine_name, h.os_version,\n                h.first_seen as \"first_seen!\", h.last_seen as \"last_seen!\", h.is_active as \"is_active!\"\n            FROM hardware h\n            INNER JOIN licenses l ON l.hardware_id = h.id\n            WHERE l.admin_id = $1\n            ORDER BY h.last_seen DESC\n            ",
